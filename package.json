{
  "name": "p_kate_site",
  "version": "2.4.1",
  "private": true,
  "homepage": "./",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./commitizen.config.js"
    }
  },
  "packageManager": "pnpm@10.28.2",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "bundle-stats": "react-scripts build --stats",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d dist",
    "lint": "eslint",
    "ghClear": "gh-pages-clean gh-pages -d build",
    "prepare": "husky",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS -g './commitlint.config.js'"
    }
  },
  "dependencies": {
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "vite": "^7.3.1"
  },
  "overrides": {
    "lodash": "^4.17.21",
    "tmp": "^0.2.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@eslint/js": "^9.39.2",
    "@types/react": "19.2.10",
    "@types/react-dom": "^19.2.3",
    "@types/react-scroll": "^1.8.10",
    "cz-customizable": "^7.5.1",
    "eslint": "9.39.2",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "react-responsive-carousel": "^3.2.23",
    "react-scroll": "^1.9.3",
    "sass": "^1.97.3"
  }
}